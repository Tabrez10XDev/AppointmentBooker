<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dashboard_user_graph"
    app:startDestination="@id/slotViewUser">

    <fragment
        android:id="@+id/slotViewUser"
        android:name="com.example.NIC_Dungarpur.fragments.SlotViewUser"
        android:label="fragment_slot_view_user"
        tools:layout="@layout/fragment_slot_view_user" >
        <action
            android:id="@+id/action_slotViewUser_to_userDetailsForm"
            app:destination="@id/userDetailsForm" />
        <action
            android:id="@+id/action_slotViewUser_to_viewDetailsForm"
            app:destination="@id/viewDetailsForm" />
    </fragment>
    <fragment
        android:id="@+id/userDetailsForm"
        android:name="com.example.NIC_Dungarpur.fragments.UserDetailsForm"
        android:label="fragment_user_details_form"
        tools:layout="@layout/fragment_user_details_form" />
    <fragment
        android:id="@+id/viewDetailsForm"
        android:name="com.example.NIC_Dungarpur.fragments.ViewDetailsForm"
        android:label="ViewDetailsForm" />
</navigation>